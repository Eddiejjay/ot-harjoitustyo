<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reseptixapp.AllRecipesController">
   <children>
      <ListView fx:id="listView" layoutX="114.0" layoutY="90.0" onEditCancel="#updateListView" onEditCommit="#updateListView" onEditStart="#updateListView" prefHeight="200.0" prefWidth="373.0" />
      <Button fx:id="letsCookButton" layoutX="114.0" layoutY="284.0" mnemonicParsing="false" onAction="#letsCookButtonClicked" prefHeight="25.0" prefWidth="373.0" text="Let's cook" />
      <Label layoutX="182.0" layoutY="31.0" prefHeight="31.0" prefWidth="207.0" text="ReseptiX" textFill="#0a0a0a">
         <font>
            <Font size="45.0" />
         </font></Label>
      <Button fx:id="addRecipe" layoutX="462.0" layoutY="337.0" mnemonicParsing="false" onAction="#addRecipeClicked" prefHeight="51.0" prefWidth="81.0" text="Add recipe" />
      <Button fx:id="surpriseButton" layoutX="389.0" layoutY="57.0" mnemonicParsing="false" onAction="#surpriseButtonClicked" prefHeight="25.0" prefWidth="100.0" text="Surprise me" />
      <Button fx:id="test" layoutX="22.0" layoutY="350.0" mnemonicParsing="false" onAction="#testButtonClicked" text="testButton" />
      <Button fx:id="makeAmenuButton" layoutX="325.0" layoutY="337.0" mnemonicParsing="false" onAction="#makeAmenuButtonClicked" prefHeight="51.0" prefWidth="129.0" text="Make a menu" />
